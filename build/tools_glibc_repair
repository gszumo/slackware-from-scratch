#!/bin/bash
#*****************************
tools_glibc_repair () {
#*****************************
	case $GLIBCVER in
		2.33 )
			cd /tools/lib
			cp -v libc-$GLIBCVER.so.backup libc-$GLIBCVER.so
			ln -sf libc-$GLIBCVER.so libc.so.6
			rm libc-$GLIBCVER.so.backup
			cd $SFS/scripts ;;
		2.34 )
			echo ;;
	esac
}

tools_glibc_repair
