#!/bin/bash

build_gcc-post () {
#******************************************************************
# Uninstall gnat-gpl after gnat-ada package build
#
# Note: Much of this script is copied from the LFS manual.
#       Copyright Â© 1999-2022 Gerard Beekmans and may be
#       copied under the MIT License.
#******************************************************************
# set -xv
export PATH=$PATH_HOLD
rm -rf /opt/gnat

if [ -f gcc.SlackBuild.old ]; then
	mv -v gcc.SlackBuild{.old,}
fi

cd /scripts || exit 1
}

build_gcc-post
