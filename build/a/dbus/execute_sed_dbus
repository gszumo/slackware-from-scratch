#!/bin/bash

execute_sed_dbus () {
#******************************************************************
# delete "--enable-x11-autolaunch" line in SlackBuild
#******************************************************************
if [ ! -f dbus.SlackBuild.old ]; then
	cp -v dbus.SlackBuild dbus.SlackBuild.old
	(
		cd "$SLACKSRC"/a/dbus || exit 1
		sed -i -e "/--enable-x11-autolaunch/d" dbus.SlackBuild
	)
fi
}
execute_sed_dbus
